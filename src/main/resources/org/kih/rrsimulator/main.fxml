<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.StackedBarChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="650.0" prefWidth="1062.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kih.rrsimulator.Controller">
   <children>
      <VBox prefHeight="650.0" prefWidth="399.0">
         <children>
            <TableView fx:id="processesView" prefHeight="675.0" prefWidth="499.0">
               <columns>
                  <TableColumn editable="false" prefWidth="88.0" resizable="false" style="-fx-alignment: center" text="Process ID" />
                  <TableColumn editable="false" prefWidth="91.0" resizable="false" style="-fx-alignment: center" text="Arrival Time" />
                  <TableColumn editable="false" prefWidth="93.0" resizable="false" style="-fx-alignment: center" text="Service Time" />
                  <TableColumn prefWidth="87.0" text="Delete" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="517.0">
               <children>
                  <Spinner fx:id="pidSpinner" editable="true">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Spinner>
                  <Spinner fx:id="arrivalTimeSpinner">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Spinner>
                  <Spinner fx:id="serviceTimeSpinner">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Spinner>
                  <Button fx:id="addButton" mnemonicParsing="false" prefHeight="21.0" prefWidth="150.0" text="Add" textAlignment="CENTER" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Spinner fx:id="timeQuantumSpinner" prefHeight="21.0" prefWidth="171.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Spinner>
                  <Button mnemonicParsing="false" prefHeight="21.0" prefWidth="346.0" text="Simulate">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox prefHeight="650.0" prefWidth="718.0">
         <children>
             <StackedBarChart fx:id="stackedBarChart" alternativeColumnFillVisible="true" legendVisible="false" prefHeight="697.0" prefWidth="691.0">
                 <xAxis>
                     <NumberAxis autoRanging="false" forceZeroInRange="false" minorTickCount="1" side="TOP" tickLength="1.0" tickUnit="1.0" upperBound="10.0" />
                 </xAxis>
                 <yAxis>
                     <CategoryAxis side="LEFT" />
                 </yAxis>
             </StackedBarChart>
            <HBox alignment="CENTER" prefHeight="213.0" prefWidth="691.0">
               <children>
                  <HBox prefHeight="214.0" prefWidth="609.0" />
                  <Pane prefHeight="77.0" prefWidth="83.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
